<MudAppBar Elevation="2">
    <MudIconButton Icon="@Icons.Material.Filled.Menu" Color="Color.Inherit" Edge="Edge.Start" OnClick="@OnToggleClick" />
    <MudSpacer />
    <MudIconButton Icon="@Icons.Custom.Brands.GitHub" Color="Color.Inherit" href="https://github.com/MudBlazor/MudBlazor" Target="_blank" />
</MudAppBar>

@code
{
    [Parameter]
    public EventCallback OnToggle { get; set; }

    private async void OnToggleClick()
    {
        await OnToggle.InvokeAsync();
    }
}
